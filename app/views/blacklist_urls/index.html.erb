<h1>Listing blacklist_urls</h1>

<table>
  <thead>
    <tr>
      <th>Domain</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @blacklist_urls.each do |blacklist_url| %>
      <tr>
        <td><%= blacklist_url.domain %></td>
        <td><%= link_to 'Show', blacklist_url %></td>
        <td><%= link_to 'Edit', edit_blacklist_url_path(blacklist_url) %></td>
        <td><%= link_to 'Destroy', blacklist_url, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

